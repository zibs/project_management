<div class="row">
  <div class="form-group-lg col-md-6 col-md-offset-3">
    <h1 class="title text-lg-center">Favourites</h1>
      <table class="table table-striped">

      <% if @favourites %>

        <% @favourites.each do |fav| %>
        <tr>
          <%= binding.pry %>
          <td><%= link_to "#{fav.title}", project_path(fav.id) %></td>
        </tr>
        <% end %>
      <% end %>
    </table>
  </div>
</div>
